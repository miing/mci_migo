    <wadl:resource_type id="account">
      
      <wadl:method name="GET" id="account-get">
        <wadl:response>
          <wadl:representation
    href="@@SERVICE_ROOT@@/#account-full"/>
          <wadl:representation
    mediaType="application/xhtml+xml" id="account-xhtml"/>
          <wadl:representation
    mediaType="application/vnd.sun.wadl+xml"
    id="account-wadl"/>
        </wadl:response>
      </wadl:method>

      <wadl:method name="PUT" id="account-put">
        <wadl:request>
          <wadl:representation
    href="@@SERVICE_ROOT@@/#account-full"/>
        </wadl:request>
      </wadl:method>

      <wadl:method name="PATCH" id="account-patch">
        <wadl:request>
          <wadl:representation
    href="@@SERVICE_ROOT@@/#account-diff"/>
        </wadl:request>
      </wadl:method>

      

    </wadl:resource_type>


