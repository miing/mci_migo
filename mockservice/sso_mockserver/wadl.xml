            </wadl:param>
            <wadl:param style="query" required="true"
                        name="openid">
             <wadl:doc xmlns="http://www.w3.org/1999/xhtml">
OpenID Identifier of the account
</wadl:doc>
              
            </wadl:param>
          
        </wadl:request>
        
      </wadl:method>
      <wadl:method id="authentications-validate_email"
                   name="GET">
        <wadl:doc xmlns="http://www.w3.org/1999/xhtml">
Validate email by sending token user received in email
</wadl:doc>
        <wadl:request>
          
            <wadl:param style="query" name="ws.op"
                        required="true"
                        fixed="validate_email">
              <wadl:doc>The name of the operation being invoked.</wadl:doc>
            </wadl:param>
            <wadl:param style="query" required="true"
                        name="email_token">
             <wadl:doc xmlns="http://www.w3.org/1999/xhtml">
Email validation token.
</wadl:doc>
              
            </wadl:param>
          
        </wadl:request>
        
      </wadl:method>
      <wadl:method id="authentications-account_by_email"
                   name="GET">
        <wadl:doc xmlns="http://www.w3.org/1999/xhtml">
Retrieve account information based on email address
</wadl:doc>
        <wadl:request>
          
            <wadl:param style="query" name="ws.op"
                        required="true"
                        fixed="account_by_email">
              <wadl:doc>The name of the operation being invoked.</wadl:doc>
            </wadl:param>
            <wadl:param style="query" required="true"
                        name="email">
             <wadl:doc xmlns="http://www.w3.org/1999/xhtml">
Email address of the account
</wadl:doc>
              
            </wadl:param>
          
        </wadl:request>
        
      </wadl:method>
      <wadl:method id="authentications-list_tokens"
                   name="GET">
        <wadl:doc xmlns="http://www.w3.org/1999/xhtml">
Get the currently valid tokens for a given user
</wadl:doc>
        <wadl:request>
          
            <wadl:param style="query" name="ws.op"
                        required="true" fixed="list_tokens">
              <wadl:doc>The name of the operation being invoked.</wadl:doc>
            </wadl:param>
            <wadl:param style="query" required="true"
                        name="consumer_key">
             <wadl:doc xmlns="http://www.w3.org/1999/xhtml">
User's OpenID identifier
</wadl:doc>
              
            </wadl:param>
          
        </wadl:request>
        
      </wadl:method>
      <wadl:method id="authentications-invalidate_token"
                   name="POST">
        <wadl:doc xmlns="http://www.w3.org/1999/xhtml">
Make the given token invalid
</wadl:doc>
        <wadl:request>
          <wadl:representation
    mediaType="application/x-www-form-urlencoded">
            <wadl:param style="query" name="ws.op"
                        required="true"
                        fixed="invalidate_token">
              <wadl:doc>The name of the operation being invoked.</wadl:doc>
            </wadl:param>
            <wadl:param style="query" required="true"
                        name="token">
             <wadl:doc xmlns="http://www.w3.org/1999/xhtml">
The token you want to invalidate
</wadl:doc>
              
            </wadl:param>
            <wadl:param style="query" required="true"
                        name="consumer_key">
             <wadl:doc xmlns="http://www.w3.org/1999/xhtml">
The consumer key (openid identifier)
</wadl:doc>
              
            </wadl:param>
          </wadl:representation>
        </wadl:request>
        
      </wadl:method>
