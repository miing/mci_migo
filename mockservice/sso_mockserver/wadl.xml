        <wadl:doc xmlns="http://www.w3.org/1999/xhtml">
Retrieve account information based on OpenID identifier
</wadl:doc>
        <wadl:request>
          
            <wadl:param style="query" name="ws.op"
                        required="true"
                        fixed="account_by_openid">
