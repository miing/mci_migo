             <wadl:doc xmlns="http://www.w3.org/1999/xhtml">
Token name.
</wadl:doc>
              
            </wadl:param>
          
        </wadl:request>
        
      </wadl:method>
      <wadl:method id="authentications-team_memberships"
                   name="GET">
        <wadl:doc xmlns="http://www.w3.org/1999/xhtml">
Query user for team memberships
</wadl:doc>
        <wadl:request>
          
            <wadl:param style="query" name="ws.op"
                        required="true"
                        fixed="team_memberships">
